<!DOCTYPE html>
<html>
<head>
  <title>Tet</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id='container'>
    <div id='header'>
      <%= render 'layouts/header' %>
    </div>
    <div id='content'>
      <% flash.each do |key, msg| %>
        <%= content_tag :div, msg, :id => key %>
      <% end %>
      <%= yield %>
    </div>
    <div id='footer'>
      <%= render 'layouts/footer' %>
    </div>
  </div>
</body>
</html>
